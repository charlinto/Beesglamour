<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>stay in touch</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
            <li class="breadcrumb-item active">stay in touch</li>
          </ol>
        </div>
      </div>
    </div><!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="row">
      <div class="col-12">
        <div class="card">
          <div class="card-header">
            <h3 class="card-title">stay in touch</h3>
          </div>
          <!-- /.card-header -->
          <div class="card-body">
        
            <table id="example2" class="table table-bordered table-hover">
              <thead>
                <tr>
                  <th>User Email</th>
                  <th>Date</th>
                  <th>Delete</th>
                  
                </tr>
              </thead>
              <tbody>
                {{#each email}}
                <tr>
                  <td>{{this.userEmail}}</td>
                  
                  <td>{{this.created_at}}</td>
              
                  <td>
                   <a href="/getintouchs/delete/{{this.id}}"
                        class="btn btn-danger text-white btn-sm">Delete</a>
                  </td>
                </tr>
                {{/each}}
              </tbody>
             <tfoot>
                                <tr>
                                    <th>Rendering engine</th>
                                    <th>Browser</th>
                                    <th>Date</th>
                                    <th>CSS grade</th>
                                </tr>
                            </tfoot>
            </table>
          </div>
          <!-- /.card-body -->
        </div>
        <!-- /.card -->

        <!-- /.card -->
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->
  </section>
  <!-- /.content -->
</div>