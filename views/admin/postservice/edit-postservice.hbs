<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Add Postservice</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
            <li class="breadcrumb-item active">Postservice</li>
          </ol>
        </div>
      </div>
    </div><!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <!-- left column -->
        <div class="col-md-12">
          <!-- jquery validation -->
          <div class="card card-primary">
            <!-- /.card-header -->
            <!-- form start -->

            {{> messages}}
            <form role="form" id="quickForm" method="POST" action="/postservice/update/{{postservice.id}}">
              <div class="card-body">
                <div class="form-group">
                  <label for="exampleInputEmail1">Postservice Name</label>
                  <input type="text" name="postservice_name" class="form-control" id="exampleInputEmail1"
                    placeholder="Enter category" value="{{postservice.postserviceName}}">
                </div>
               
          
                <div class="form-group">
                  <label for="exampleInputDescription">Post Description</label>
                  <div class="row">
                    <textarea name="postservice_desc" id="" class="form-control" rows="5">{{post.postDesc}}</textarea>
                  </div>
                </div>
                <div class="form-group mb-0">
                </div>
              </div>
              <!-- /.card-body -->
              <div class="card-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </form>
          </div>
          <!-- /.card -->
        </div>
        <!--/.col (left) -->
        <!-- right column -->
        <div class="col-md-6">

        </div>
        <!--/.col (right) -->
      </div>
      <!-- /.row -->
    </div><!-- /.container-fluid -->
  </section>
  <!-- /.content -->
</div>